{
  "name": "tsuyoshi_tech",
  "version": "1.0.0",
  "description": "My home page",
  "author": "Tsuyoshi Yamaguchi",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts dev --modern=client",
    "build": "nuxt-ts build --modern=client",
    "analyze": "nuxt-ts build --modern=client --analyze",
    "generate": "nuxt-ts generate --modern=client",
    "serve": "nuxt-ts serve",
    "start": "yarn generate && nuxt-ts serve",
    "precommit": "npm run lint",
    "lint": "eslint 'app/**/*.{ts,vue}'",
    "test": "jest",
    "test:e2e": "jest -c app/e2e/jest.config.js app/e2e --runInBand",
    "test:percy": "percy exec -- yarn test:e2e",
    "format": "prettier --write 'app/**/*.{ts,vue}'",
    "lhci": "lhci autorun",
    "modernizr": "npx modernizr -c modernizr-config.json -d ./app/static/libs/modernizr.min.js",
    "webpc": "webpc"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "1.0.0",
    "@nuxtjs/sitemap": "2.4.0",
    "normalize.css": "8.0.1",
    "nuxt": "2.14.3",
    "ts-node": "9.0.0",
    "vue-property-decorator": "9.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/preset-env": "7.11.0",
    "@nuxt/typescript-build": "2.0.2",
    "@nuxtjs/eslint-config-typescript": "3.0.0",
    "@percy/puppeteer": "1.1.0",
    "@types/jest": "26.0.10",
    "@types/node": "13.13.15",
    "@types/puppeteer": "3.0.1",
    "@vue/test-utils": "1.0.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.3.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.4.1",
    "jest-puppeteer": "4.4.0",
    "jest-serializer-vue": "2.0.2",
    "node-sass": "4.14.1",
    "prettier": "2.0.5",
    "pug": "3.0.0",
    "pug-plain-loader": "1.0.0",
    "sass-loader": "9.0.3",
    "ts-jest": "26.2.0",
    "ts-loader": "8.0.2",
    "typescript": "3.9.7",
    "vue-class-component": "7.2.5",
    "vue-eslint-parser": "7.1.0",
    "vue-jest": "3.0.6",
    "vue-svg-inline-loader": "1.5.0",
    "webp-converter-cli": "0.0.5"
  }
}
