{
  "name": "tsuyoshi_tech",
  "version": "1.0.0",
  "description": "My home page",
  "author": "Tsuyoshi Yamaguchi",
  "private": true,
  "scripts": {
    "dev": "nuxt dev --modern=client",
    "build": "nuxt build --modern=client",
    "analyze": "nuxt build --modern=client --analyze",
    "generate": "nuxt generate --modern=client",
    "serve": "nuxt serve",
    "start": "yarn generate && nuxt serve",
    "precommit": "npm run lint",
    "lint": "eslint './**/*.{ts,vue}'",
    "test": "jest",
    "test:e2e": "jest -c app/e2e/jest.config.js app/e2e --runInBand",
    "test:percy": "percy exec -- yarn test:e2e",
    "format": "prettier --write './**/*.{ts,vue}'",
    "lhci": "lhci autorun",
    "modernizr": "npx modernizr -c modernizr-config.json -d ./app/static/libs/modernizr.min.js",
    "webpc": "webpc",
    "bundlesize": "bundlesize"
  },
  "dependencies": {
    "@nuxtjs/sitemap": "2.4.0",
    "normalize.css": "8.0.1",
    "nuxt": "2.15.2",
    "ts-node": "9.1.1",
    "vue-property-decorator": "9.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.13.8",
    "@babel/preset-env": "7.13.8",
    "@nuxt/types": "2.15.2",
    "@nuxt/typescript-build": "2.0.6",
    "@nuxtjs/eslint-config-typescript": "5.0.0",
    "@percy/puppeteer": "1.1.0",
    "@types/jest": "26.0.20",
    "@types/node": "13.13.45",
    "@types/puppeteer": "5.4.3",
    "@vue/test-utils": "1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.6.3",
    "bundlesize": "0.18.1",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "jest-puppeteer": "4.4.0",
    "jest-serializer-vue": "2.0.2",
    "node-sass": "5.0.0",
    "prettier": "2.2.1",
    "pug": "3.0.2",
    "pug-plain-loader": "1.1.0",
    "sass-loader": "10.1.1",
    "ts-jest": "26.5.2",
    "ts-loader": "8.0.17",
    "typescript": "4.2.2",
    "vue-class-component": "7.2.6",
    "vue-eslint-parser": "7.5.0",
    "vue-jest": "3.0.7",
    "vue-svg-inline-loader": "2.1.2",
    "webp-converter-cli": "0.0.5"
  }
}
